{
  "ignore-bin-package": false,
  "skip-missing": false,
  "ignore-dirs": [
    "dist",
    "build",
    "coverage",
    "node_modules",
    ".git"
  ],
  "ignore-patterns": [
    "*.d.ts",
    "*.test.ts",
    "*.spec.ts"
  ],
  "parsers": {
    "*.ts": "typescript",
    "*.tsx": "typescript",
    "*.js": "es6",
    "*.jsx": "jsx"
  },
  "detectors": [
    "requireCallExpression",
    "importDeclaration",
    "importCallExpression",
    "exportDeclaration"
  ],
  "specials": [
    "eslint",
    "prettier",
    "typescript",
    "jest",
    "husky",
    "lint-staged",
    "vite",
    "vitest",
    "postcss",
    "tailwindcss",
    "autoprefixer",
    "daisyui",
    "tsx",
    "nodemon"
  ],
  "ignores": [
    "@types/*",
    "rimraf",
    "typescript",
    "eslint",
    "prettier",
    "husky",
    "lint-staged",
    "concurrently",
    "npm-check-updates",
    "depcheck",
    "audit-ci",
    "syncpack",
    "asynckit",
    "bcrypt-pbkdf",
    "combined-stream",
    "typedarray-to-buffer",
    "vscode-jsonrpc",
    "websocket",
    "tcode-shared",
    "express",
    "ws",
    "ssh2",
    "node-pty",
    "dotenv",
    "@grpc/grpc-js",
    "protobufjs",
    "@microsoft/dev-tunnels-connections",
    "@microsoft/dev-tunnels-contracts",
    "@microsoft/dev-tunnels-management",
    "nodemon",
    "tsx",
    "@xterm/addon-fit",
    "@xterm/xterm",
    "postcss-import",
    "preact",
    "@preact/preset-vite",
    "@vitest/coverage-v8",
    "@vitest/ui",
    "autoprefixer",
    "daisyui",
    "postcss",
    "tailwindcss",
    "vite",
    "vite-plugin-pwa",
    "vitest",
    "workbox-window",
    "open",
    "tcode-server"
  ]
}
